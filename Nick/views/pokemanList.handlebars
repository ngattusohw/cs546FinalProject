
<div class="login-padding">
  <form action="/pokemon/getPokemon" method="POST" id="login-form">
        <input type="text" name="pokename" class="form-control" placeholder="Pokemon Name">
        <button type="submit" class="submit-button">Search!</button>
  </form>
</div>


<table style="width:50%" id="student-table">
  <tr>
    <th>Student UserName</th>
    <th>Student Password</th> 
  </tr>
{{#each pokemon}}
   <tr>
    <td>{{this.url}}</td>
    <td>{{this.name}}</td> 
  </tr>
{{else}}
	<td> No moves :( </td>
  <td> Please add students! </td> 
{{/each}}
</table>